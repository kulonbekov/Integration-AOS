<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

    <module name="electronic-navigation-seals" package="com.axelor.apps.ens.db"/>

    <entity name="CustomsOffice">
        <integer name="code" title="Code"/>                       <!--код-->
        <string name="name" title="Name"/>                        <!--наименование-->
        <string name="fullName" title="Full name"/>               <!--полное наименование-->
        <many-to-one name="countryCode" title="Country code"
                     ref="com.axelor.apps.base.db.Country"/>      <!--код страны-->
    </entity>

</domain-models>
